{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "no-console": 0,
      "no-unused-vars": 1,
      "indent": ["error", 2],
      "no-multi-spaces": ["error"],
      "no-undef": "off",
      "no-empty-pattern": "off"
  }
}